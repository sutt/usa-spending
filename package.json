{
  "name": "usa-spending-poc",
  "version": "0.1.0",
  "description": "Proof-of-concept for fetching federal contract awards from USAspending API",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "fetch:award": "ts-node src/index.ts fetch:award",
    "fetch:transaction": "ts-node src/index.ts fetch:transaction",
    "fetch": "ts-node src/index.ts fetch:award",
    "analyze": "ts-node src/index.ts analyze",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["usaspending", "federal-contracts", "api-client"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "commander": "^12.0.0",
    "date-fns": "^3.3.1",
    "js-yaml": "^4.1.0"
  }
}
